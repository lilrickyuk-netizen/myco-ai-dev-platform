# Execution Engine Environment Variables

# Server Configuration
EXECUTION_ENGINE_PORT=8001
NODE_ENV=development

# Security
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5173

# Redis Configuration (for job queue)
REDIS_URL=redis://localhost:6379/2

# Docker Configuration
DOCKER_SOCKET=/var/run/docker.sock

# Rate Limiting
EXECUTION_RATE_LIMIT_WINDOW=60000
EXECUTION_RATE_LIMIT_MAX=10

# Job Configuration
MAX_CONCURRENT_JOBS=5
JOB_TIMEOUT=30000
JOB_MEMORY_LIMIT=128m
JOB_CPU_LIMIT=0.5

# Supported Languages Configuration
SUPPORTED_LANGUAGES=javascript,typescript,python,java,cpp,rust,go

# Security Settings
ENABLE_NETWORK_ISOLATION=true
RUN_AS_USER=nobody

# Monitoring
ENABLE_HEALTH_CHECKS=true
HEALTH_CHECK_INTERVAL=30000

# Logging
LOG_LEVEL=info
ENABLE_DETAILED_LOGS=false