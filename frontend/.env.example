# Frontend Environment Variables

# API Configuration
VITE_API_URL=http://localhost:3000
VITE_AI_ENGINE_URL=http://localhost:8000
VITE_EXECUTION_ENGINE_URL=http://localhost:8001
VITE_WS_URL=ws://localhost:3000

# Authentication
VITE_CLERK_PUBLISHABLE_KEY=your-clerk-publishable-key

# Development
NODE_ENV=development

# Feature Flags
VITE_ENABLE_COLLABORATION=true
VITE_ENABLE_AI_ASSISTANT=true
VITE_ENABLE_ADVANCED_TEMPLATES=true
VITE_ENABLE_DEPLOYMENT=true
VITE_ENABLE_AGENTS=true

# Editor Configuration
VITE_EDITOR_THEME=vs-dark
VITE_EDITOR_FONT_SIZE=14

# Monitoring (Optional)
VITE_ENABLE_ANALYTICS=false
VITE_SENTRY_DSN=your-sentry-dsn

# Build Configuration
VITE_BUILD_TARGET=esnext
VITE_BUILD_MINIFY=false