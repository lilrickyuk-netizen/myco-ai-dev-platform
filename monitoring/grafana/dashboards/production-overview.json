{
  "dashboard": {
    "id": null,
    "title": "Myco AI Platform - Production Overview",
    "tags": ["myco-ai", "production", "overview"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Service Health Status",
        "type": "stat",
        "targets": [
          {
            "expr": "service_health{service=\"myco-ai-platform\"}",
            "legendFormat": "Service Health"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 1,
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "green", "value": 1}
              ]
            },
            "mappings": [
              {"options": {"0": {"text": "Unhealthy"}}, "type": "value"},
              {"options": {"1": {"text": "Healthy"}}, "type": "value"}
            ]
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Request Rate (per second)",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(http_requests_total{service=\"myco-ai-platform\"}[5m])",
            "legendFormat": "Requests/sec"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 100},
                {"color": "red", "value": 1000}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "Error Rate (%)",
        "type": "stat",
        "targets": [
          {
            "expr": "http_request_error_rate{service=\"myco-ai-platform\"} * 100",
            "legendFormat": "Error Rate"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 5}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0}
      },
      {
        "id": 4,
        "title": "P95 Response Time",
        "type": "stat",
        "targets": [
          {
            "expr": "http_request_duration_seconds_p95{service=\"myco-ai-platform\"} * 1000",
            "legendFormat": "P95 Latency"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 1000},
                {"color": "red", "value": 2000}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0}
      },
      {
        "id": 5,
        "title": "Request Volume",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(http_requests_total{service=\"myco-ai-platform\"}[5m])",
            "legendFormat": "Requests/sec"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "fillOpacity": 10
            }
          }
        },
        "gridPos": {"h": 9, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 6,
        "title": "Response Time Trends",
        "type": "timeseries",
        "targets": [
          {
            "expr": "http_request_duration_seconds_p95{service=\"myco-ai-platform\"} * 1000",
            "legendFormat": "P95 Latency (ms)"
          },
          {
            "expr": "avg(rate(http_request_duration_seconds_sum{service=\"myco-ai-platform\"}[5m]) / rate(http_request_duration_seconds_count{service=\"myco-ai-platform\"}[5m])) * 1000",
            "legendFormat": "Average Latency (ms)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 2
            }
          }
        },
        "gridPos": {"h": 9, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 7,
        "title": "Memory Usage",
        "type": "timeseries",
        "targets": [
          {
            "expr": "nodejs_memory_usage_bytes{service=\"myco-ai-platform\",type=\"heapUsed\"} / 1024 / 1024",
            "legendFormat": "Heap Used (MB)"
          },
          {
            "expr": "nodejs_memory_usage_bytes{service=\"myco-ai-platform\",type=\"heapTotal\"} / 1024 / 1024",
            "legendFormat": "Heap Total (MB)"
          },
          {
            "expr": "nodejs_memory_usage_bytes{service=\"myco-ai-platform\",type=\"rss\"} / 1024 / 1024",
            "legendFormat": "RSS (MB)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "MB",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 2
            }
          }
        },
        "gridPos": {"h": 9, "w": 12, "x": 0, "y": 17}
      },
      {
        "id": 8,
        "title": "Error Rate Over Time",
        "type": "timeseries",
        "targets": [
          {
            "expr": "http_request_error_rate{service=\"myco-ai-platform\"} * 100",
            "legendFormat": "Error Rate (%)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "fillOpacity": 20
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 5}
              ]
            }
          }
        },
        "gridPos": {"h": 9, "w": 12, "x": 12, "y": 17}
      },
      {
        "id": 9,
        "title": "System Uptime",
        "type": "stat",
        "targets": [
          {
            "expr": "process_uptime_seconds{service=\"myco-ai-platform\"}",
            "legendFormat": "Uptime"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "custom": {
              "displayMode": "basic"
            }
          }
        },
        "gridPos": {"h": 4, "w": 8, "x": 0, "y": 26}
      },
      {
        "id": 10,
        "title": "Total Requests",
        "type": "stat",
        "targets": [
          {
            "expr": "http_requests_total{service=\"myco-ai-platform\"}",
            "legendFormat": "Total Requests"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "custom": {
              "displayMode": "basic"
            }
          }
        },
        "gridPos": {"h": 4, "w": 8, "x": 8, "y": 26}
      },
      {
        "id": 11,
        "title": "SLO Compliance",
        "type": "table",
        "targets": [
          {
            "expr": "http_request_duration_seconds_p95{service=\"myco-ai-platform\"} <= 2",
            "legendFormat": "P95 Latency SLO",
            "format": "table",
            "instant": true
          },
          {
            "expr": "http_request_error_rate{service=\"myco-ai-platform\"} <= 0.01",
            "legendFormat": "Error Rate SLO",
            "format": "table",
            "instant": true
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {"options": {"0": {"text": "❌ Breach"}}, "type": "value"},
              {"options": {"1": {"text": "✅ Compliant"}}, "type": "value"}
            ]
          }
        },
        "gridPos": {"h": 4, "w": 8, "x": 16, "y": 26}
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "30s",
    "schemaVersion": 30,
    "version": 1,
    "editable": true,
    "graphTooltip": 1,
    "links": []
  }
}